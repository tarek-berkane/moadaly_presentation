{
  "name": "slidev-theme-penguin",
  "version": "0.0.0-development",
  "description": "A Penguin theme for Slidev",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev>",
  "keywords": [
    "slidev-theme",
    "slidev",
    "slidev-penguin"
  ],
  "license": "MIT",
  "private": false,
  "homepage": "https://penguin-theme.alvarosaburido.dev/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvarosaburido/slidev-theme-penguin.git"
  },
  "bugs": {
    "url": "https://github.com/alvarosaburido/slidev-theme-penguin/issues"
  },
  "slidev": {
    "colorSchema": "both",
    "highlighter": "prism",
    "default": {
      "aspectRatio": "16/9",
      "fonts": {
        "sans": "Nunito",
        "title": "Gilroy",
        "mono": "Fira Code"
      }
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "scripts": {
    "dev": "slidev example.md --open",
    "build": "slidev build example.md",
    "export": "slidev export example.md",
    "semantic-release": "semantic-release",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.27.15",
    "codemirror-theme-vars": "^0.1.1",
    "prism-theme-vars": "^0.2.2",
    "theme-vitesse": "^0.1.13",
    "vite-svg-loader": "^3.1.0"
  },
  "devDependencies": {
    "@slidev/cli": "0.27.15",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.0.3",
    "playwright-chromium": "^1.17.1",
    "semantic-release": "^18.0.0"
  },
  "main": ".eslintrc.js"
}
